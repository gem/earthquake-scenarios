<?xml version='1.0' encoding='UTF-8'?>
<nrml>

<!--From NAF hazard model:
 	Tectonic_Type_B is prescribed for transition zones of intermediate characteristics between SCC and ASC.
	https://gitlab.openquake.org/hazard/mosaic/NAF/-/blob/master/in/gmmLT.xml -->
	<logicTree logicTreeID="">
		<logicTreeBranchSet branchSetID="bs_01" uncertaintyType="gmpeModel" applyToTectonicRegionType="Tectonic_Type_B">
			<logicTreeBranch branchID="b_01_02">
				<uncertaintyModel>ChiouYoungs2014</uncertaintyModel>
				<uncertaintyWeight>0.25000</uncertaintyWeight>
			</logicTreeBranch>
			<logicTreeBranch branchID="b_01_03">
				<uncertaintyModel>AkkarEtAlRjb2014</uncertaintyModel>
				<uncertaintyWeight>0.25000</uncertaintyWeight>
			</logicTreeBranch>
			<logicTreeBranch branchID="b_01_04">
                <uncertaintyModel>                
                    [ModifiableGMPE]
                    gmpe.AtkinsonBoore2006Modified2011 = {}
                    add_between_within_stds.with_betw_ratio = 1.8
                </uncertaintyModel>
				<uncertaintyWeight>0.25000</uncertaintyWeight>
			</logicTreeBranch>
			<logicTreeBranch branchID="b_01_05">
                <uncertaintyModel>                
                    [ModifiableGMPE]
                    gmpe.PezeshkEtAl2011NEHRPBC.NRCan15SiteTerm = {}
                    add_between_within_stds.with_betw_ratio = 1.8
                </uncertaintyModel>
				<uncertaintyWeight>0.25000</uncertaintyWeight>
			</logicTreeBranch>
		</logicTreeBranchSet>
	</logicTree>
</nrml>


